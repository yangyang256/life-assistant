# 必填
LECTURE_URL=https://sojump.cn-hangzhou.log.aliyuncs.com/logstores/activitypostdata/track

# 多个 token 用英文逗号分隔；可有空格，这段代码会 strip 掉
# 引号可写可不写（python-dotenv 会自动处理）
ACCESS_TOKENS="tokenA123, tokenB456, tokenC789"

# 必填：从抓包里拿到的两个 ID
PREFILED_ID=your_preFiled_id_here
AUDIENCE_ID=your_audience_id_here

# 必填：抢票目标时间（北京时间，UTC+8）
GRAB_TIME=2025-11-11 18:00:00

# 选填：并发和重试参数（不填用默认）
THREADS_PER_ACCOUNT=3
MAX_RETRIES=10
SLEEP_RETRY=0.08

# 选填：抢到后回调通知地址（不需要就留空或删掉）
NOTIFY_WEBHOOK=
